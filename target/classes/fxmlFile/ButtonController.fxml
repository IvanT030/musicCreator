<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="detailVBox" xmlns:fx="http://javafx.com/fxml" fx:controller="org.example.AudioController">
    <children>
        <Label text="Adjust Speed:" />
        <Slider fx:id="speedSlider" min="0.1" max="3.0" value="1.0" showTickLabels="true" showTickMarks="true" majorTickUnit="0.5" blockIncrement="0.1" />
        <HBox spacing="10">
            <Button text="Confirm" onAction="#handleConfirm" />
            <Button text="Cancel" onAction="#handleCancel" />
        </HBox>
    </children>
</VBox>